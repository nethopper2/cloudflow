apiVersion: v1
kind: Service
metadata:
  labels:
    app: frontend
    service: frontend
  name: frontend
spec:
  externalTrafficPolicy: Cluster
  internalTrafficPolicy: Cluster
  ipFamilies:
    - IPv4
  ipFamilyPolicy: SingleStack
  ports:
    - nodePort: 31000
      port: 8080
      protocol: TCP
      targetPort: 8080
  selector:
    app: frontend-v1
  sessionAffinity: None
  type: NodePort